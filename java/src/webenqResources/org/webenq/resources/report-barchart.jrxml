	<style name="table_colored_cells" style="Crosstab Data Text" mode="Opaque" fontSize="7">
		<conditionalStyle>
			<conditionExpression><![CDATA[(
$P{COLOR_RANGE_MEAN5}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("lowRed") &&
$V{average}<(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("highRed")
)||
(
$P{COLOR_RANGE_MEAN10}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("lowRed") &&
$V{average}<(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("highRed")
)]]></conditionExpression>
			<style mode="Opaque" forecolor="#FFFFFF" backcolor="#FF0000">
				<box>
					<leftPen lineWidth="0.25" lineStyle="Solid" lineColor="#000000"/>
					<rightPen lineWidth="0.25" lineStyle="Solid" lineColor="#000000"/>
				</box>
			</style>
		</conditionalStyle>
		<conditionalStyle>
			<conditionExpression><![CDATA[(
$P{COLOR_RANGE_MEAN5}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("lowYellow") &&
$V{average}<(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("highYellow")
)||
(
$P{COLOR_RANGE_MEAN10}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("lowYellow") &&
$V{average}<(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("highYellow")
)]]></conditionExpression>
			<style mode="Opaque" forecolor="#000000" backcolor="#FFFF00">
				<box>
					<leftPen lineWidth="0.25" lineStyle="Solid" lineColor="#000000"/>
					<rightPen lineWidth="0.25" lineStyle="Solid" lineColor="#000000"/>
				</box>
			</style>
		</conditionalStyle>
		<conditionalStyle>
			<conditionExpression><![CDATA[(
$P{COLOR_RANGE_MEAN5}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("lowGreen") &&
$V{average}<=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean5")).get("highGreen")
)||
(
$P{COLOR_RANGE_MEAN10}.indexOf($F{report_question_id} )>-1 &&
$V{average} >=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("lowGreen") &&
$V{average}<=(Double)((Map)$P{COLOR_RANGE_MAP}.get("mean10")).get("highGreen")
)]]></conditionExpression>
			<style mode="Opaque" forecolor="#000000" backcolor="#00FF00">
